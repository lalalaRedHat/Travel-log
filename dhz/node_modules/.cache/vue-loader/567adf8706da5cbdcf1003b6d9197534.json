n            </router-link>\r\n          <!-- 单一日志信息结束 -->\r\n          </div>\r\n          <van-divider contentPosition=\"center\" >\r\n            已经没有了\r\n          </van-divider>\r\n          <!-- 单一日志信息结束 -->\r\n        </lazy-component>\r\n\r\n\r\n\r\n            </van-tab>\r\n        </van-tabs>\r\n\r\n               \r\n        \r\n    </div>\r\n</div>\r\n</template>\r\n<style scope>\r\n    .personage{\r\n        width: 100%;\r\n    }\r\n    .personage .navb{\r\n        background: transparent;\r\n    }\r\n    .background{\r\n        width: 100%;height: 300px;\r\n        background:\r\n         url('../assets/common/myPersonage_top.jpg')\r\n           repeat ;\r\n          background-size: 100%\r\n    }\r\n    .background .image{\r\n      padding: 40px 100px;\r\n      text-align: center;\r\n    }\r\n    .background .pname{\r\n        width: 200px;\r\n        font-size: 1rem;\r\n        color:rgba(27, 29, 27, 0.904);\r\n        padding: 150px  10px;\r\n    }\r\n    .background .man{\r\n        margin: -30px 310px;\r\n    }\r\n    .background .grade{\r\n        width: 100px;\r\n        margin: 10px 30px;\r\n    }\r\n    .background .personage  .van-tabs__line{\r\n    background-color: #5ABCC8;\r\n  }\r\n</style>\r\n<script>\r\n\r\nexport default {\r\n  data() {\r\n    return {\r\n      activeName: 'a',\r\n      show_border:false,\r\n       fileList: [],\r\n       // 是否吸顶\r\n      isFixed:true,\r\n      art:{},\r\n      aa:''\r\n    }\r\n  },\r\n  mounted(){\r\n      let phone = this.$route.params.phone;\r\n     //获取服务器传的手机号的值\r\n   this.axios.get('/journal/individual?phone=18753108178').then(res=>{\r\n     this.art=res.data.articleInfo;\r\n    // console.log(res.data.articleInfo);\r\n    // console.log(this.art);\r\n    })\r\n    this.aa=this.art.phone;\r\n  },\r\n   methods:{\r\n    onClickLeft() {\r\n      this.$router.push(\"/\");\r\n    },\r\n     onClickRight() {\r\n      this.$router.push(\"/login\");\r\n    },\r\n    }\r\n};\r\n\r\n</script>\r\n"]}]}